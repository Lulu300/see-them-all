{
    "log": {
        "path": "/var/log/see_them_all.log",
        "level": "DEBUG",
        "max_size": 1000000,
        "backup_count": 1,
        "show_on_console": true
    },
    "inputs": {
        "plex_1": {
            "enabled": true,
            "type": "inputs.plex.Plex",
            "sync_all": false,
            "url": "",
            "token": "",
            "users": [""],
            "video_types": ["episode"]
        }
    },
    "outputs": {
        "tvtime": {
            "enabled": true,
            "type": "outputs.tvtime.Tvtime",
            "inputs": ["plex_1"],
            "token_file": "tvt_token"
        },
        "simkl": {
            "enabled": true,
            "type": "outputs.simkl.Simkl",
            "inputs": ["plex_1"],
            "client_id": "",
            "token_file": "simkl_token"
        },
        "discord": {
            "enabled": true,
            "type": "outputs.discord.Discord",
            "inputs": ["plex_1"],
            "channel": ,
            "uid": "",
            "token": ""
        }
    },
    "cache_folder": "/tmp/see_them_all"
}
